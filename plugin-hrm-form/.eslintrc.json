{
  "extends": [
    "twilio-react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "no-console": "off",
    "no-warning-comments": "off",
    "no-confusing-arrow": "off",
    "no-alert": "off",
    "import/no-extraneous-dependencies": "off",
    "jsx-a11y/tabindex-no-positive": "off"
  }
}